#include<iostream>
using namespace std;
int main()
{
   int n1=0,n2=1,nt,i,n,sum=0;
   cout<<"enter n:";
   cin>>n;
   cout<<"fibonnaci series is:";
   cout<<n1<<" "<<n2<<" ";
      	for(i=2;i<=n*2;i++)
   	{
	   nt = n1 + n2;
	   n1 = n2;
	   n2 = nt;
	   cout<<nt<<" ";
  	if(i%2==0)
   sum = sum + nt;
  }
  cout<<"sum is:"<<sum;
   
}
