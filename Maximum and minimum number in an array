#include<iostream>
using namespace std;
int main()
{
	int n,a[10],min,max,i;
	cout<<"enter n:";
	cin>>n;
	cout<<"elements are:";
	for(i=0;i<n;i++)
	{
	cin>>a[i];
    }
	max=a[0];
	min=a[0];
	for(i=1;i<n;i++)
	{
		if(a[i]>max)
		max=a[i];
		if(a[i]<min)
		min=a[i];
	}
	
	cout<<"maximum element in an array is:"<<max<<endl;
	cout<<"minimum element in an array is:"<<min<<endl;
}
