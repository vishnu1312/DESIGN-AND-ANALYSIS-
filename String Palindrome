#include<iostream>
#include<string.h>
using namespace std;
main()
{
	char str1[29],str2[20];
	int i,j,len =0,flag = 0;
	cout<<"enter striing:";
	gets(str1);
	len = strlen(str1)-1;
	for(i=len,j=0;i>=0;i--,j++)
	str1[j]=str2[i];
	if(strcmp(str1,str2))
	flag = 1;
	if(flag==1)
	cout<<str1<<"is not palindrome";
	else
	cout<<str1<<"is palindrome";
}
